VERSION : 0.1
   VAR_TEMP 
      Consenso_A_Riempitrice : Bool;
   END_VAR


BEGIN
	REGION Virtuale
	    REGION Parametri
	        "Valvola".V["K_Valvola_UV"].Prm.SensorLess := True;
	    END_REGION ;
	    REGION Gestione
	        "GestValvola"(K_Valvola := "K_Valvola_UV",
	                      K_TipoValvola := "K_TipoValvola_Monostabile",
	                      K_Zona := "K_Zona_Sterilizzatore");
	    END_REGION ;
	    REGION Uscite
	        #Consenso_A_Riempitrice := "Valvola".V["K_Valvola_UV"].Out.CmdPos;
	    END_REGION ;            
	END_REGION ;
END_FUNCTION

